@inject Menhera.Intefaces.IBoardCollection BoardCollection

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset='utf-8'>
    <link rel="stylesheet" href="/css/site.css">
    <link rel="stylesheet" href="/css/ban.css">
    <link rel="stylesheet" href="/css/banned.css">
</head>
<body>
<aside>
    <nav>
        <ul>
            @foreach (var board in BoardCollection.Boards)
            {
                if (!board.IsHidden)
                {
                    <li>
                        <a asp-controller="Board" asp-action="Board" asp-route-prefix="@board.Prefix">
                            /@board.Prefix/@board.Postfix
                        </a>
                    </li>
                }
            }
        </ul>
    </nav>
</aside>
@RenderBody()
</body>
</html>